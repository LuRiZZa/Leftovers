<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Leftover"
             x:Class="Leftover.MainPage">
            

    <StackLayout>
        <Label x:Name="UsernameLbl" Text="Username" />
        <Entry x:Name="UsernameEntry" Placeholder="Username" />
        <Label x:Name="PasswordLbl" Text="Password" />
        <Entry x:Name="PasswordEntry" Placeholder="Password" />
        <Button x:Name="LoginBtn" Text="Login" Clicked="LoginClick" />
    </StackLayout>


    


</ContentPage>